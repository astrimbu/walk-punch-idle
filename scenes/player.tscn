[sub_resource type="GDScript" id="GDScript_8smjq"]
script/source = "extends CharacterBody2D

@export var speed := 40
@onready var animation_player := $AnimationPlayer
@onready var camera := $Camera2D
@onready var sprite := $Sprite2D

var target : Vector2
var is_moving = false
var mouse_held = false
var last_direction : Vector2 = Vector2.ZERO

func _ready():
	target = global_position

func _input(event):
	if event is InputEventKey:
		is_moving = false
		_play_punch_animation()
		
	if event is InputEventMouseButton:
		if event.button_index == MOUSE_BUTTON_LEFT and event.pressed:
			mouse_held = true
			is_moving = true
			target = get_global_mouse_position()
			_play_walk_animation(target)
		elif event.button_index == MOUSE_BUTTON_LEFT and not event.pressed:
			mouse_held = false

func _physics_process(delta):
	if mouse_held:
		target = get_global_mouse_position()
		_play_walk_animation(target)

	if is_moving:
		var direction = (target - global_position).normalized()
		last_direction = direction
		velocity = direction * speed
		move_and_slide()
		for i in get_slide_collision_count():
			var collision = get_slide_collision(i)
			if !mouse_held:
				_play_idle_animation()
				is_moving = false

		if direction.x < 0:
			sprite.scale.x = -1
		elif direction.x > 0:
			sprite.scale.x = 1

		if global_position.distance_to(target) < 5 and not mouse_held:
			is_moving = false
			velocity = Vector2.ZERO
			_play_idle_animation()
	else:
		if !animation_player.is_playing(): 
			_play_idle_animation()

func _play_walk_animation(target: Vector2):
	var direction = _get_direction(target - global_position)
	print(\"Walk direction: \", direction)
	
	if direction == 90:
		animation_player.play(\"walkto\")
	elif direction == 45 or direction == 135:
		animation_player.play(\"walkto45\")
	elif direction == 0 or direction == 180 or direction == -180:
		animation_player.play(\"walk\")
	elif direction == -45 or direction == -135:
		animation_player.play(\"walkaway45\")
	elif direction == -90:
		animation_player.play(\"walkaway\")

func _play_idle_animation():
	var direction = _get_direction(last_direction)
	print(\"Idle direction: \", direction)
	
	if direction == 90:
		animation_player.play(\"idleto\")
	elif direction == 45 or direction == 135:
		animation_player.play(\"idleto45\")
	elif direction == 0 or direction == 180 or direction == -180:
		animation_player.play(\"idle\")
	elif direction == -45 or direction == -135:
		animation_player.play(\"idleaway45\")
	elif direction == -90:
		animation_player.play(\"idleaway\")

func _play_punch_animation():
	var direction = _get_direction(last_direction)
	print(\"Punch direction: \", direction)
	
	if direction == 90:
		animation_player.play(\"punchto\")
	elif direction == 45 or direction == 135:
		animation_player.play(\"punchto45\")
	elif direction == 0 or direction == 180 or direction == -180:
		animation_player.play(\"punch\")
	elif direction == -45 or direction == -135:
		animation_player.play(\"punchaway45\")
	elif direction == -90:
		animation_player.play(\"punchaway\")

func _get_direction(vector: Vector2) -> int:
	var angle = vector.angle()
	var degrees = rad_to_deg(angle)
	return int(round(degrees / 45) * 45)
"

